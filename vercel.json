{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "env": {
    "NEXTAUTH_SECRET": "@NEXTAUTH_SECRET",
    "FERGEH_ENCRYPTION_KEY": "@FERGEH_ENCRYPTION_KEY",
    "GOOGLE_CLIENT_ID": "@GOOGLE_CLIENT_ID",
    "GOOGLE_CLIENT_SECRET": "@GOOGLE_CLIENT_SECRET"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
